{
    "name": "chrisdicarlo/laravel-production-refactor",
    "description": "",
    "keywords": [
        "chrisdicarlo",
        "laravel-production-refactor"
    ],
    "homepage": "https://github.com/chrisdicarlo/laravel-production-refactor",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Chris Di Carlo",
            "email": "chris@dicarlosystems.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ChrisDiCarlo\\LaravelProductionRefactor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChrisDiCarlo\\LaravelProductionRefactor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChrisDiCarlo\\LaravelProductionRefactor\\LaravelProductionRefactorServiceProvider"
            ]
        }
    }
}
